{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "tags",
  "info": {
    "name": "tag"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "is_active": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "contacttag": {
      "plugin": "crm-plugin",
      "model": "contacttag",
      "via": "tag"
    }
  }
}
